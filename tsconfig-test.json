{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "paths": {
      "test/*": ["./test/*"]
    }
  },
  "include": ["test/**/*.ts", "test/**/*.js", "test/**/.*.js", "src/**/.*.js", "*.ts", "*.js", ".*.js"]
}
